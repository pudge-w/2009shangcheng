// 首页轮播图
.index .van-swipe__indicator
  width 20px
  height 3px
  border-radius 0

// 首页搜索栏
.index 
  .van-search
    position absolute
    z-index 999
    width 100%
    color #fff
  .van-search__content
    background rgba(255, 255, 255, .2)
  .van-field__control::placeholder
    color #fff
  .van-cell
    color #fff